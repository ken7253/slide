{
  "name": "slidev-theme-starter",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^52.2.4"
  },
  "devDependencies": {
    "@slidev/cli": "^52.8.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "dark",
    "defaults": {
      "htmlAttrs": {
        "lang": "ja"
      },
      "fonts": {
        "sans": "Inter, M PLUS 2",
        "mono": "M PLUS 1 Code",
        "weights": "400,600,700,900"
      }
    }
  }
}
